class Solution:
    # @return a string
    def longestPalindrome(self, s):
    
        s = [c.lower() for l in s if s.isalnum]
        longest=''
        
        for i in range(len(s)):
            for j in range(len(s)):
                if s[i] == s[j]:
                    if isPalindrome(''.join(i:j+1)):
                        if j-i+1 > len(longest):
                            longest = ''.join(i:j+1)